{
    "RunBenchmark": true,
    "BenchmarkPath": "./benchmarks/samples",
    "InstancePath": "./benchmarks/samples/example.qdimacs",
    "OutputFileName": "results",
    "SolverOptions": {
        "SolverType": "cdcl",
        "LiteralSelection": "vss",
        "Preprocess": true,
        "UniversalReduction": true,
        "PureLiteralDeletion": true,
        "Restarts": true,
        "PreResolution": false,
        "PreResolutionConfig": {
            "min_ratio": 0.25,
            "max_ratio": 0.5,
            "max_clause_length": "infinity",
            "repeat_above": 3,
            "iterations": 1
        }
    }
}